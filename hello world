a = "12345zx67890c"
a = a.replace("1", "H")
a = a.replace("2", "e")
a = a.replace("3", "l")
a = a.replace("4", "l")
a = a.replace("5", "o")
a = a.replace("z", ",")
a = a.replace("x", " ")
a = a.replace("6", "W")
a = a.replace("7", "o")
a = a.replace("8", "r")
a = a.replace("9", "l")
a = a.replace("0", "d")
a = a.replace("c", "!")
print(a)
